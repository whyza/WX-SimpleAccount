.acctype, .date, .dateandtype {
  height: 50px;
}

.pickercenter {
  padding: 16px;
}

.item {
  width: 20%;
  text-align: center;
  margin-bottom: 5px;
}

.container {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  padding: 2% 1% 1% 1%;
  /* height: 200px; */
}

.active {
  color: #1cbbb4;
  background: #fff;
}

.choseClassify {
  position: relative;
  padding-bottom: 10px;
}

.choseClassify .dots {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 25rpx;
  display: flex;
  justify-content: center;
}

.choseClassify .dots .dot {
  margin: 0 8rpx;
  width: 14rpx;
  height: 14rpx;
  background: #ddd;
  border-radius: 8rpx;
  transition: all 0.6s;
}

.choseClassify .dots .dot.active {
  width: 24rpx;
  background: #1cbbb4;
}

.typeheader {
  position: relative;
  height: 50px;
}

.typeImage2 {
  width: 40px;
  height: 40px;
  position: absolute;
  top: 2px;
  left: 20px;
}

.iconbgc2, .iconbgcw2 {
  width: 40px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  border-radius: 40px;
}
