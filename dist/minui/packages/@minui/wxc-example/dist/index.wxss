.example {}
.example--wrap {
    padding: 0 10rpx
}
.example__head {
    padding: 30rpx 30rpx 0;
    text-align: center
}
.example__head--title {
    display: inline-block;
    padding: 0 40rpx 10rpx 40rpx;
    font-size: 32rpx;
    color: #9B9B9B
}
.example__head--line {
    margin: 0 auto;
    width: 150rpx;
    height: 2rpx;
    background-color: #D8D8D8
}
.example__head--desc {
    padding-top: 13rpx;
    color: #BEBEBE;
    font-size: 26rpx
}
.example__foot {
    margin: 100rpx 0 30rpx 0;
    text-align: center;
    color: #1aad19;
    font-size: 0
}
.example__foot--icon {
    width: 152rpx;
    height: 23rpx
}