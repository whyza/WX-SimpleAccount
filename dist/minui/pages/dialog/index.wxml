<view>
    <!--<layout-head></layout-head>-->

    <!-- wxp template -->
    <example title="dialog" desc="弹出对话框" _pid="{{_pid}}">

        <example-demo title="标题+单行内容+取消按钮+确认按钮" desc="告知当前状态，信息和解决方法" _pid="{{_pid}}" source="{{__code__.demoBtns}}">
            <demo-btns _pid="{{_pid}}"></demo-btns>
        </example-demo>

        <example-demo title="标题+单行内容+确认按钮" desc="告知当前状态，信息和解决方法" _pid="{{_pid}}" source="{{__code__.demoDefault}}">
            <demo-default _pid="{{_pid}}"></demo-default>
        </example-demo>

        <example-demo title="标题+内容+额外内容+确认按钮" desc="这里可能是说明文字，并且支持自定义的额外内容" _pid="{{_pid}}" source="{{__code__.demoExtra}}">
            <demo-extra _pid="{{_pid}}"></demo-extra>
        </example-demo>

        <example-demo title="标题+图片+单行内容+确认按钮" desc="内容可以展示图片" _pid="{{_pid}}" source="{{__code__.demoImage}}">
            <demo-image _pid="{{_pid}}"></demo-image>
        </example-demo>

        <example-demo title="换行内容+确认按钮" desc="多行文本" _pid="{{_pid}}" source="{{__code__.demoMoreline}}">
            <demo-moreline _pid="{{_pid}}"></demo-moreline>
        </example-demo>

        <example-demo title="黑色内容+确认按钮" desc="文案颜色可自定义" _pid="{{_pid}}" source="{{__code__.demoNotitle}}">
            <demo-notitle _pid="{{_pid}}"></demo-notitle>
        </example-demo>

        <example-md content="{{__code__.readme}}" _pid="{{_pid}}"></example-md>
    </example>

    <!--<layout-foot></layout-foot>-->
</view>