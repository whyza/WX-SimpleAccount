<view>
    <!--<layout-head></layout-head>-->

    <!-- wxp template -->
    <example title="wxc-input" desc="输入框组件" _pid="{{_pid}}">

        <example-demo title="基础用法" desc="" _pid="{{_pid}}" source="{{__code__.demoDefault}}">
            <demo-default _pid="{{_pid}}"></demo-default>
        </example-demo>

        <example-demo title="无标题输入框" desc="" _pid="{{_pid}}" source="{{__code__.demoNoTitle}}">
            <demo-no-title _pid="{{_pid}}"></demo-no-title>
        </example-demo>

        <example-demo title="圆角输入框" desc="" _pid="{{_pid}}" source="{{__code__.demoRadius}}">
            <demo-radius _pid="{{_pid}}"></demo-radius>
        </example-demo>

        <example-demo title="号码输入框，带校验" desc="" _pid="{{_pid}}" source="{{__code__.demoNumber}}">
            <demo-number _pid="{{_pid}}"></demo-number>
        </example-demo>

        <example-demo title="带icon的输入框" desc="" _pid="{{_pid}}" source="{{__code__.demoIcon}}">
            <demo-icon _pid="{{_pid}}"></demo-icon>
        </example-demo>
        <example-md content="{{__code__.readme}}" _pid="{{_pid}}"></example-md>
    </example>

