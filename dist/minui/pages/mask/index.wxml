<view>
    <!--<layout-head></layout-head>-->

    <!-- wxp template -->
    <example title="mask" desc="遮罩层" _pid="{{_pid}}">

        <example-demo title="基础实例" desc="基础实例" _pid="{{_pid}}" source="{{__code__.demoDefault}}">
            <demo-default _pid="{{_pid}}"></demo-default>
        </example-demo>

        <example-demo title="设置透明度" desc="opacity 属性" _pid="{{_pid}}" source="{{__code__.demoOpacity}}">
            <demo-opacity _pid="{{_pid}}"></demo-opacity>
        </example-demo>

        <example-demo title="设置背景色" desc="background-color 属性" _pid="{{_pid}}" source="{{__code__.demoBgcolor}}">
            <demo-bgcolor _pid="{{_pid}}"></demo-bgcolor>
        </example-demo>

        <example-demo title="设置子节点" desc="支持添加自定义的子节点" _pid="{{_pid}}" source="{{__code__.demoContent}}">
            <demo-content _pid="{{_pid}}"></demo-content>
        </example-demo>

        <example-demo title="设置子节点定位" desc="content-align 属性" _pid="{{_pid}}" source="{{__code__.demoContentAlign}}">
            <demo-content-align _pid="{{_pid}}"></demo-content-align>
        </example-demo>

        <example-demo title="只能通过回调来关闭遮罩层" desc="设置 locked 属性值为 true" _pid="{{_pid}}" source="{{__code__.demoLocked}}">
            <demo-locked _pid="{{_pid}}"></demo-locked>
        </example-demo>

        <example-md content="{{__code__.readme}}" _pid="{{_pid}}"></example-md>
    </example>

    <!--<layout-foot></layout-foot>-->
</view>