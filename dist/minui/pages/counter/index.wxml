<view>
    <!--<layout-head></layout-head>-->

    <!-- wxp template -->
    <example title="counter" desc="数字框" _pid="{{_pid}}">
        <example-demo title="默认" desc="默认计数器展示" _pid="{{_pid}}" source="{{__code__.demoDefault}}">
            <demo-default _pid="{{_pid}}"></demo-default>
        </example-demo>

        <example-demo title="不可更改数量" desc="禁止更改计数器展示" _pid="{{_pid}}" source="{{__code__.demoDisable}}">
            <demo-disable _pid="{{_pid}}"></demo-disable>
        </example-demo>

        <example-demo title="设置最大值和操作符号颜色" desc="设置最大值的计数器展示" _pid="{{_pid}}" source="{{__code__.demoMax}}">
            <demo-max _pid="{{_pid}}"></demo-max>
        </example-demo>
        <example-demo title="设置最小值" desc="设置最小值的计数器展示" _pid="{{_pid}}" source="{{__code__.demoMin}}">
            <demo-min _pid="{{_pid}}"></demo-min>
        </example-demo>

        <example-md content="{{__code__.readme}}" _pid="{{_pid}}"></example-md>
    </example>

    <!--<layout-foot></layout-foot>-->
</view>